        android:supportsRtl="true"
        android:theme="@style/Theme.Medhub">
        <activity android:name=".PatientUpdatePassword"></activity>
        <activity android:name=".HospitalUpdatePassword" />
        <activity android:name=".PatientEditProfile" />
        <activity android:name=".HospitalEditProfile" />
        <activity android:name=".HospitalProfile">
        </activity>
        <activity android:name=".PatientProfile">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HospitalRegister" />
        <activity android:name=".PatientRegister" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>